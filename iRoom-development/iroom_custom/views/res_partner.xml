<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>
        <record id="iroom_custom_view_partner_form" model="ir.ui.view">
            <field name="name">iroom_custom_view_partner_form</field>
            <field name="model">res.partner</field>
            <field name="inherit_id" ref="base.view_partner_form"/>
            <field name="arch" type="xml">
                <xpath expr="//group[@name='acc_agent']" position="after">
                    <group string="Supplier" name="supplier">
                        <field name="is_supplier"/>
                    </group>
                </xpath>
            </field>
        </record>
    </data>
</odoo>