<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>
        <record id="ntmp_room_type_view_tree" model="ir.ui.view">
            <field name="name">ntmp_room_type_view_tree</field>
            <field name="model">ntmp.room.type</field>
            <field name="arch" type="xml">
                <tree editable="bottom">
                    <field name="code"/>
                    <field name="name"/>
                    <field name="name_en"/>
                </tree>
            </field>
        </record>
        <record id="ntmp_room_type_action" model="ir.actions.act_window">
            <field name="name">NMTP Room Types</field>
            <field name="type">ir.actions.act_window</field>
            <field name="res_model">ntmp.room.type</field>
            <field name="view_mode">tree</field>
        </record>
        <menuitem id="ntmp_room_type_view_menu" name="NMTP Rooms Types"
                  parent="ntmp_root_menu" action="ntmp_room_type_action" sequence="5"/>
    </data>
</odoo>