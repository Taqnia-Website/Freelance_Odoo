<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>
        <record id="ntmp_view_hotel_folio1_form" model="ir.ui.view">
            <field name="name">ntmp_view_hotel_folio1_form</field>
            <field name="model">hotel.folio</field>
            <field name="inherit_id" ref="hotel.view_hotel_folio1_form"/>
            <field name="arch" type="xml">
                <xpath expr="//field[@name='service_lines']/tree" position="inside">
                    <field name="ntmp_service_id" readonly="1" force_save="1"/>
                    <field name="ntmp_item_number" readonly="1" force_save="1"/>
                </xpath>
            </field>
        </record>
    </data>
</odoo>