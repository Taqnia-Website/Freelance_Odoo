<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>
        <record id="ntmp_cancel_reason_view_tree" model="ir.ui.view">
            <field name="name">ntmp_cancel_reason_view_tree</field>
            <field name="model">ntmp.cancel.reason</field>
            <field name="arch" type="xml">
                <tree editable="bottom">
                    <field name="code"/>
                    <field name="name"/>
                </tree>
            </field>
        </record>
        <record id="ntmp_cancel_reason_action" model="ir.actions.act_window">
            <field name="name">Cancel Reasons</field>
            <field name="type">ir.actions.act_window</field>
            <field name="res_model">ntmp.cancel.reason</field>
            <field name="view_mode">tree</field>
        </record>
        <menuitem id="ntmp_cancel_reason_view_menu" name="Cancel Reasons"
                  parent="ntmp_root_menu" action="ntmp_cancel_reason_action" sequence="9"/>
    </data>
</odoo>