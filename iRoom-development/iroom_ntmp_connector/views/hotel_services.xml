<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>
        <record id="ntmp_view_hotel_services_form" model="ir.ui.view">
            <field name="name">ntmp_view_hotel_services_form</field>
            <field name="model">hotel.services</field>
            <field name="inherit_id" ref="hotel.view_hotel_services_form"/>
            <field name="arch" type="xml">
                <xpath expr="//notebook" position="inside">
                    <page name="ntmp" string="NTMP">
                        <group>
                            <field name="ntmp_service_id"/>
                        </group>
                    </page>
                </xpath>
            </field>
        </record>
    </data>
</odoo>