<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>
        <record id="ntmp_folio_service_view_form" model="ir.ui.view">
            <field name="name">ntmp_folio_service_view_form</field>
            <field name="model">folio.service</field>
            <field name="inherit_id" ref="hotel_booking_folio.folio_service_view_form"/>
            <field name="arch" type="xml">
                <xpath expr="//field[@name='type']" position="after">
                    <field name="ntmp_service_id" options="{'no_open': True, 'no_edit': True, 'no_create': True}"
                        readonly="1" force_save="1"/>
                </xpath>
            </field>
        </record>
    </data>
</odoo>