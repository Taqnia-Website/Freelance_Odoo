<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="1">
        <function model="ir.config_parameter" name="set_param" eval="('sys_tab_name', 'iRoom')"/>
        <function  model="ir.default" name="set_system_favicon" eval="('res.config.settings', 'system_favicon')"/>
        <function model="ir.config_parameter" name="set_param" eval="('odoo_replacement_text', 'iRoom')"/>
        <record id="favicon_default_image" model="res.config.settings">
            <field name="system_favicon" type="base64" file="odoo_debrand_biz/static/src/img/iroom.png" />
        </record>
    </data>
    <record id="base.partner_root" model="res.partner">
        <field name="name">SystemBot</field>
        <field name="email">system@example.com</field>
        <field name="image_1920" type="base64" file="odoo_debrand_biz/static/src/img/systembot.png"/>
    </record>
</odoo>