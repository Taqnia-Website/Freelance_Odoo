<?xml version="1.0" encoding="UTF-8" ?>
<templates xml:space="preserve">
    <t t-name="UpgradeDialogInherit" t-inherit="web.UpgradeDialog" t-inherit-mode="extension" owl="1">
        <xpath expr="//Dialog" position="replace">

            <Dialog size="'md'" title="'iRoom Enterprise'">
                <div class="row" role="status">
                    <div class="col-6">
                        To Get this feature Contact Us!
                    </div>
                </div>
                <t t-set-slot="footer" owl="1">
                    <button class="btn btn-secondary" t-on-click="this.props.close">Cancel</button>
                </t>
            </Dialog>
        </xpath>
    </t>
</templates>
